                </li>
            </ul>
            <br />
            <br />
        </nav>
    </aside>
    <main class="main-wrap">
        <header class="main-header navbar">
            <div class="col-search">
                <div class="content-header mt-2 mb-0">
                    <h2 class="content-title card-title">Order List</h2>
                </div>

            </div>
            <div class="col-nav">
                <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside"><i
                        class="material-icons md-apps"></i></button>
                <ul class="nav">



                    <li class="dropdown nav-item">
                        <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdownAccount"
                            aria-expanded="false"> <img class="img-xs rounded-circle"
                                src="/assets/imgs/people/avatar-2.png" alt="User" /></a>
                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownAccount">

                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item text-danger" href="/admin/logout"><i
                                    class="material-icons md-exit_to_app"></i>Logout</a>
                        </div>
                    </li>

                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>

                </div>
                <div>
                </div>
            </div>
            <div class="card mb-4">
                
                <!-- card-header end// -->

                <% if (cartData.length> 0){%>


                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr class="text-center">
                                        <th>NO</th>
                                        <th scope="col">Id</th>
                                        <th scope="col">payment methode</th>


                                        <th scope="col">Date</th>
                                        <th scope="col">Status</th>

                                        <th scope="col" class="text-end">Action</th>
                                    </tr>
                                </thead>
                                <tbody class="text-center">
                                    <% for (let i=cartData.length-1; i>= 0; i--) { %>

                                        <% console.log(cartData[i].products)%>
                                            <tr>
                                                <td>
                                                    <b>
                                                        <%=cartData.length-i%>
                                                    </b>
                                                </td>
                                                <td>
                                                    <%=cartData[i].orderId%>
                                                </td>
                                                <td>
                                                    <%= cartData[i].payment%>
                                                </td>
                                                <td>
                                                    <%=cartData[i].orderDate.toLocaleDateString('en-US', {
                                                        weekday: 'short' , year: 'numeric' , month: 'short' ,
                                                        day: 'numeric' })%>
                                                </td>



                                                <% if (cartData[i].orderStatus==="canceled" ) { %>
                                                    <td><span class="badge rounded-pill alert-danger">Canceled</span>
                                                    </td>
                                                    <% } else { %>
                                                        <!-- <td><span class="badge rounded-pill alert-warning">Pending</span></td>   -->
                                                        <td><span
                                                                class="badge rounded-pill alert-success">Received</span>
                                                        </td>
                                                        <% } %>


                                                            <td class="text-end">
                                                                <a href="orderdetailes?id=<%=cartData[i]._id%>"
                                                                    class="btn btn-md rounded font-sm">Detail</a>

                                                            </td>

                                            </tr>
                                            <%}%>








                                </tbody>
                            </table>
                        </div>
                        <!-- table-responsive //end -->
                    </div>
                    <%}else{%>
                        <p class="p-5 text-center">no orders</p>


                        <%}%>
                            <!-- card-body end// -->
            </div>
            <!-- card end// -->



            <div class="row mt-5 ">
                <div class="col text-center">
                    <div class="block-27">
                            <div class="pagination-area mt-15 mb-50">
                                <nav aria-label="Page navigation example">
                                    <% if (cartData.length !== 0) { %>
                                        <ul class="pagination justify-content-center">
                                            <% for (let i = 1; i <= totalDoc / 3; i++) { %>
                                                <li class="page-item <%= page == i ? 'active' : '' %>">
                                                    <a class="page-link" href="/admin/orders?page=<%= i %>">
                                                        <%= i %>
                                                    </a>
                                                </li>
                                            <% } %>
                                        </ul>
                                    <% } %>
                                </nav>
                            </div>
                    </div>
                </div>
            </div>








        </section>
        <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                        document.write(new Date().getFullYear());
                    </script>
                    &copy; Nest - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">All rights reserved</div>
                </div>
            </div>
        </footer>
    </main>
