	<section class="ftco-section ftco-cart">
		<div class="container" id="checkout-relode">
			<div class="row">
				<div class="container-fluid col-lg-12 col-md-12 col-sm-12 carttable" id="cart-relod-divs">
					
					
					<% if (!wishlist || !wishlist.product || wishlist.product.length === 0) { %>
						<div class="empty-cart">
							<h5 class="empty-head ">empty Wishlist</h5>
							<img src="/images/9960436.jpg" alt="" class="cart-img">
							<p class="mt-3">your Wishlist is empty</p>
							<a href="/" class="btn cart-btn"><span class="">fill your Wishlist</span></a>
						</div>
						<%}else{%>
							<div class="cart-list" id="cart-relod-div">
								<table class="table">
									<thead class="thead-primary">
										<tr class="text-center">
											<th>&nbsp;</th>
											<th>&nbsp;</th>
											<th>Product</th>
											<th>Stock</th>
											<th>Price</th>
											<th></th>
											
										</tr>
									</thead>
									<tbody class="">
										<% wishlist.product.forEach(function(product, index) { %>

											<tr class="text-center">
												<td class="product-remove"><a
														onclick="removewishlist('<%=product._id %>')"><span
															class="ion-ios-close"></span></a>
												</td>

												<td class="image-prod">
													<div class="img-cart">
		
														<img src="/sharpimg/<%= product.productId.images.image1%>"
															alt="" class="w-100 h-100">
													</div>
												</td>

												<td class="product-name w-25">
													<h3>
														<%=product.productId.name%>
													</h3>
													<p class="product-discription">
														<%=product.productId.description%>
													</p>
												</td>
												<% if (product.productId.quantity===0) { %>
													<td>
														<p>Out of Stock</p>
													</td>
													<% } else if (product.productId.quantity> 5) { %>
														<td>
															Only <%= product.productId.quantity %> left
														</td>
														<% } else { %>
															<td>
																<%= product.productId.quantity %> available
															</td>
															<% } %>

															

																	<% if (!product.productId.offer) { %>


																		<td class="price">₹<%=product.productId.price%></td>

																			<% } else { %>

																			<td class="price">₹<%=product.productId. discountedPrice%></td>


					
																						<% } %>








																</td>

																<td>
																	
																	<p class="bottom-area d-flex px-3">
																		<a type="button" class="add-to-cart text-center py-2 mr-1" onclick="addcart('<%=product.productId._id%>')"><span>Add to
																				cart <i class="ion-ios-add ml-1"></i></span></a>
																				</p>
																</td>

																
											</tr>
											<% }); %>
									</tbody>
								</table>
							</div>
							<% }; %>
				</div>
			</div>
		</div>
	</section>

