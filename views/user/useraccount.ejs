                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="table-responsive border card w-100 text-center">
                                        <% if (user.walletHistory && user.walletHistory.length> 0) { %>
                                            <table class="table ">
                                                <thead>
                                                    <tr>
                                                        <th scope="col">#</th>
                                                        <th scope="col">Date</th>

                                                        <th scope="col">Amount</th>
                                                        <th scope="col">Type</th>
                                                    </tr>
                                                </thead>
                                                <tbody>

                                                    <% user.walletHistory.forEach(function(walletdata,index) { %>

                                                        <tr>
                                                            <th scope="row">
                                                                <%=1+index%>
                                                            </th>
                                                            <td>
                                                                <%=walletdata.date.toISOString().split('T')[0]%>
                                                            </td>


                                                            <% if(walletdata.amount> 0 ) {%>
                                                                <td style="color: #6B8E23;">
                                                                    <%=walletdata.amount%>
                                                                </td>
                                                                <% }else{ %>
                                                                    <td style="color: #CB192C;">
                                                                        <%=walletdata.amount%>
                                                                    </td>
                                                                    <%}%>




                                                                        <% if(walletdata.amount> 0 ) {%>
                                                                            <td
                                                                                style="color: #6B8E23; font-weight: 900;">
                                                                                Credited</td>
                                                                            <% }else{ %>
                                                                                <td
                                                                                    style="color: #CB192C; font-weight: 700;">
                                                                                    Debited</td>
                                                                                <%}%>

                                                                                    <%})%>

                                                        </tr>

                                                        <% } else { %>
                                                            <p>no wallet history</p>
                                                            <% } %>
                                                </tbody>
                                            </table>
                                    </div>



                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

